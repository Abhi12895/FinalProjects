package com.tcs.ioc;

//it supports  two types of injections
//->Setter injection
//->Constructor injection
public class A {
     public void method() {
    	 System.out.println("from A()");
     }
     
     
     public void myInit() {
    	 System.out.println(getClass() + "init()");
     }
     public void myDestroy() {
    	 System.out.println(getClass() + "destroy()");
     }
}
